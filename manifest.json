{
    "manifest_version": 3,
    "name": "Download video from pinterest",
    "version": "0.1",
    "description": "Download video from pinterest",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "background": {
      
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon-16.png",
        "36": "icons/icon-36.png",
        "48": "icons/icon-48.jpg",
        "128": "icons/icon-128.png"
      }
    },
    "icons": {
      "16": "icons/icon-16.png",
      "36": "icons/icon-36.png",
      "48": "icons/icon-48.jpg",
      "128": "icons/icon-128.png"
    }
}
  